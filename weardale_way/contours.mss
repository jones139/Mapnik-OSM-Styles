@sans:"Arial Regular","Liberation Sans Regular","DejaVu Sans Book";

#contours {
   line-color:#9cb197;
   line-width:0.5;

   text-fill:#9cb197;
   text-name:"[height]";
   text-face-name:@sans;
   text-character-spacing:1;
   text-size:5;
   text-allow-overlap:false;
   text-halo-radius:1;
   text-halo-fill:rgba(255,255,255,0.75);
   text-placement:line;
}



#landuse {
   [landuse="residential"]
   {
      polygon-fill: #b0b0b0;
   }
   [landuse="industrial"]
   {
      polygon-fill: #a0a0a0;
   }
   [landuse="forest"]
   {
      polygon-fill: #a0d0a0;
   }
}

#leisure-poly {
   [leisure="park"]
   {
      polygon-fill: #a0c0a0;
   }
}

#building-poly {
   polygon-fill: #101010;
    /*text-placement:line;*/
    text-face-name:@serif;
    text-name:"[name:en]";
    text-size:5;
}

#amenity-poly {
   [amenity="parking"]
   {
      polygon-fill:#000000;
   }

}
@sans:"Arial Regular","Liberation Sans Regular","DejaVu Sans Book";

#contours10 {
   line-color:#9cb197;
   line-width:0.5;

   text-fill:#9cb197;
   text-name:"[height]";
   text-face-name:@sans;
   text-size:5;
   text-allow-overlap:true;
   text-halo-radius:0.25;
   text-halo-fill:rgba(255,255,255,0.75);
   text-placement:line;
   text-min-distance:20;
   text-max-char-angle-delta:10;
}

#contours50 {
   line-color:#9cb197;
   line-width:1;

   text-fill:#9cb197;
   text-name:"[height]";
   text-face-name:@sans;
   text-size:5;
   text-allow-overlap:true;
   text-halo-radius:0.25;
   text-halo-fill:rgba(255,255,255,0.75);
   text-placement:line;
   text-min-distance:20;
   text-max-char-angle-delta:10;
}


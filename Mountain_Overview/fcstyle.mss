.FCRoads {

       	   ::outline {     line-cap:square; 
   	     	   line-join:round;
   	     	  line-color:#000; 
		  line-width:5; }
   	   line-join:round;
   	   line-color:#0000ff;
   	   line-width:3;
	   text-name:"[ROADCLASSI]";
	   text-placement:line;
	   text-face-name:@serif;

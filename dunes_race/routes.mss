#routes {
     ::outline { 
     line-cap:square; 
     line-color:rgba(255,255,255,0.5); 
     line-width:6; 
     line-dasharray:2,1;
     }
     line-color:rgba(0,0,255,0.5);
     line-width:4;

    text-name:"[name]";
    text-face-name:@serif;
    text-size:10;
    text-wrap-width:12;
    text-allow-overlap:false;
    text-halo-radius:1;
    text-halo-fill:rgba(255,255,255,0.75);
}
